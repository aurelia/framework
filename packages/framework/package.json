      "aurelia-pal": "^1.0.0",
      "aurelia-path": "^1.0.0",
      "aurelia-task-queue": "^1.0.0",
      "aurelia-templating": "^1.0.0"
    },
    "devDependencies": {
      "aurelia-pal-browser": "^1.0.0-rc.1.0.0",
      "babel": "babel-core@^5.8.24",
      "babel-runtime": "^5.8.24",
      "core-js": "^2.0.3"
    }
  },
