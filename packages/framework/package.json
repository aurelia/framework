    "karma-jasmine": "^1.1.1",
    "karma-requirejs": "^1.1.0",
    "requirejs": "^2.3.5",
    "requirejs-text": "^2.0.15",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2"
  },
